datasource db {
  provider = "postgresql"

}

generator client {
  provider = "prisma-client-js"
  engineType = "library"
}

model Record {
  id        Int      @id @default(autoincrement())
  type      String   // income | expense | remaining
  amount    Float
  reason    String
  date      DateTime
  createdAt DateTime @default(now())
}
